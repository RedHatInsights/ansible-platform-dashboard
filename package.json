{
  "name": "ansible-dashboard",
  "version": "1.0.0",
  "private": false,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@babel/types": "^7.13.17",
    "@patternfly/patternfly": "^4.219.2",
    "@patternfly/pfe-icon": "^1.12.3",
    "@patternfly/react-charts": "^6.94.11",
    "@patternfly/react-core": "^4.250.1",
    "@patternfly/react-icons": "^4.92.10",
    "@patternfly/react-tokens": "^4.93.10",
    "@redhat-cloud-services/frontend-components": "^3.7.6",
    "@redhat-cloud-services/frontend-components-config": "^4.6.4",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.5",
    "@redhat-cloud-services/frontend-components-utilities": "^3.2.12",
    "axios": "^0.26.1",
    "axios-mock-adapter": "^1.18.2",
    "classnames": "^2.2.6",
    "prop-types": "^15.8.1",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.15.7",
    "react-lines-ellipsis": "^0.15.0",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.3"
  },
  "sassIncludes": {
    "patternfly": "node_modules/patternfly/dist/sass",
    "bootstrap": "node_modules/patternfly/node_modules/bootstrap-sass/assets/stylesheets",
    "fontAwesome": "node_modules/patternfly/node_modules/font-awesome-sass/assets/stylesheets"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testURL": "http://localhost:4000/beta/ansible/ansible-dashboard"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "babel-jest": "^25.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-formatjs": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "glob": "^7.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-mock-axios": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "redux-mock-store": "^1.5.4",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "scripts": {
    "analyze": "NODE_ENV=production webpack --config config/prod.webpack.config.js --env analyze=true",
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "extract:messages": "npx @formatjs/cli extract 'src/**/*.{js,jsx}' --out-file ./translations/messages.json",
    "lint": "yarn eslint ./src",
    "prod": "NODE_ENV=production webpack serve --config config/prod.webpack.config.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "start:beta": "NODE_ENV=development BETA=true webpack serve --config config/dev.webpack.config.js",
    "test": "jest --passWithNoTests --runInBand",
    "travis:verify": "npm-run-all build lint test",
    "verify": "npm-run-all build lint test",
    "start:federated": "fec static --config config/dev.webpack.config.js"
  },
  "insights": {
    "appname": "ansible-dashboard"
  },
  "sideEffects": [
    "*.scss"
  ]
}
