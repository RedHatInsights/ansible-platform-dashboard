    "@redhat-cloud-services/frontend-components-utilities": "^4.0.17",
    "axios": "^1.7.7",
    "axios-mock-adapter": "^1.18.2",
    "classnames": "^2.2.6",
    "prop-types": "^15.8.1",
    "query-string": "^7.0.0",
